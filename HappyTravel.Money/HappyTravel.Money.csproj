<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Hasmik Harutyunyan, Oleg Konovalov, Vadim Mingazhev, Kirill Taran</Authors>
        <Description>Common package to work with money: ceiling, formatting, models</Description>
        <PackageProjectUrl>https://github.com/happy-travel/takeda</PackageProjectUrl>
        <RepositoryUrl>https://github.com/happy-travel/takeda</RepositoryUrl>
        <PackageReleaseNotes>MoneyAmount math operations overrided</PackageReleaseNotes>
        <Nullable>enable</Nullable>
        <Version>1.0.0</Version>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    </ItemGroup>

</Project>
